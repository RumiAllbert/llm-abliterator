Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"beamSearchOptimization", "[", 
    RowBox[{
    "initialGuess_", ",", "beamWidth_", ",", "maxIterations_", ",", 
     "stepSize_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"beam", ",", "candidates", ",", "scores", ",", "bestSolution"}],
