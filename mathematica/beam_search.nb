     RowBox[{
      RowBox[{"beam", "=", 
       RowBox[{"{", "initialGuess", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"candidates", "=", 
          RowBox[{"Flatten", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"beam", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "+", 
               RowBox[{"RandomReal", "[", 
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"-", "stepSize"}], ",", "stepSize"}], "}"}], ",", 
                 RowBox[{"Length", "[", "initialGuess", "]"}]}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"Length", "[", "beam", "]"}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "beamWidth"}], "}"}]}], "]"}], ",", "1"}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"scores", "=", 
          RowBox[{"lossFunction", "/@", "candidates"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"beam", "=", 
          RowBox[{"Part", "[", 
           RowBox[{"candidates", ",", 
            RowBox[{"Ordering", "[", 
             RowBox[{"scores", ",", "beamWidth"}], "]"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"bestSolution", "=", 
          RowBox[{"First", "[", "beam", "]"}]}], ";", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Mod", "[", 
             RowBox[{"iter", ",", "10"}], "]"}], "==", "0"}], ",", 
           RowBox[{"Print", "[", 
            RowBox[{
            "\"\<Iteration: \>\"", ",", "iter", ",", "\"\<, Best Loss: \>\"", 
             ",", 
             RowBox[{"lossFunction", "[", "bestSolution", "]"}]}], "]"}]}], 
          "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Max", "[", 
             RowBox[{"Abs", "[", 
              RowBox[{"Differences", "[", "beam", "]"}], "]"}], "]"}], "<", 
            RowBox[{
             RowBox[{"1", "e"}], "-", "6"}]}], ",", 
